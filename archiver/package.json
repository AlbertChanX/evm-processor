{
  "name": "evm-archiver",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "db:reset": "docker-compose down && rm -rf ./db && docker-compose up -d && rm last-indexed-block",
    "db:up": "docker-compose -f docker-compose.mongo.yml up -d",
    "db:down": "docker-compose -f docker-compose.mongo.yml down",
    "start": "ts-node src/index.ts",
    "build": "rm -rf lib && tsc",
    "start:prod": "yarn build && node lib/index",
    "typecheck": "yarn tsc --noEmit",
    "postinstall": "yarn run build"
  },
  "dependencies": {
    "indexer-utils": "0.0.1"
  }
}
