{
  "name": "evm-archiver",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "rm -rf lib && tsc",
    "start:prod": "node lib/index",
    "mongo:reset": "yarn mongo:down && rm -rf ./data && yarn mongo:up",
    "mongo:up": "docker-compose -f docker-compose.mongo.yml up -d",
    "mongo:down": "docker-compose -f docker-compose.mongo.yml down",
    "mongo:stop": "docker-compose -f docker-compose.mongo.yml stop",
    "start": "ts-node src/index.ts",
    "query-node": "ts-node src/query-node/server.ts"
  },
  "dependencies": {
    "@graphql-tools/schema": "^8.3.7",
    "archive-utils": "0.0.1",
    "axios": "^0.26.1",
    "colors": "^1.4.0",
    "dotenv": "^16.0.0",
    "evm": "^0.2.0",
    "express": "^4.17.3",
    "express-graphql": "^0.12.0",
    "graphql": "^15.8.0",
    "lodash": "^4.17.21",
    "mongoose": "^6.2.10",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.18.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/lodash": "^4.14.181",
    "nodemon": "^2.0.15",
    "ts-node": "^10.7.0"
  }
}
