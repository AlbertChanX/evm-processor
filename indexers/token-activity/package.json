{
  "name": "token-activity",
  "description": "Indexes ERC20, ERC721 & ERC1155 activity",
  "version": "0.0.1",
  "main": "lib/index.js",
  "scripts": {
    "db:reset": "docker-compose down && rm -rf ./db && docker-compose up -d && rm last-indexed-block",
    "db:up": "docker-compose up -d",
    "db:down": "docker-compose down",
    "start": "ts-node src/index.ts",
    "build": "rm -rf lib && tsc",
    "start:prod": "yarn build && node lib/index",
    "typecheck": "yarn tsc --noEmit",
    "postinstall": "yarn run build"
  },
  "dependencies": {
    "indexer-utils": "0.0.1"
  }
}
